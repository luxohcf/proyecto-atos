func_genREPO_TOTAL () {
	func_genFML_ac_reInterfazGranPublico_f
	func_genFML_ac_reEstructurasGranPublico_f
	func_genFML_ac_reGestorNegocios_f
	func_genFML_ac_reTramitaciones_f
	func_genFML_ac_reEstructurasGestorNegocios_f
	func_genFML_ac_reSpAltasGranPublico_f
	func_genFML_ac_reEstructurasAltasGranPublico_f
	func_genFML_sgce_emEmpresas_f
	func_genFML_ac_reFichaClienteRSC_f
	func_genFML_ac_reAdmGranPublico_f
	func_genFML_ac_reAdmXAGranPublico_f
	func_genFML_ac_reGesGranPublico_f
	func_genFML_ac_reGesXAGranPublico_f
	func_genFML_ac_reEstructurasEMP_f
	func_genFML_ac_reServidoresEMP_f
	func_genFML_ac_rePasarelaVX_f
	func_genFML_ac_reinterfazPasarelaVX_GF_f
	func_genFML_ac_reInterfazPasarelaM1F_f
	func_genFML_GLOBAL_BASES_F
}

if [ $# -eq 1 ]
then
	O_DIR=$ATOS_MW_FML
	case $1 in
		ac_reInterfazGranPublico_f) 
			func_genFML_ac_reInterfazGranPublico_f;;
		ac_reEstructurasGranPublico_f) 
			func_genFML_ac_reEstructurasGranPublico_f;;
		ac_reGestorNegocios_f) 
			func_genFML_ac_reGestorNegocios_f;;
		ac_reTramitaciones_f) 
