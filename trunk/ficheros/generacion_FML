# GLOBAL_BASES_F
func_genFML_GLOBAL_BASES_F () {
	echo "Generando fichero [atos_mw_servidores.def]"
	FECHA=`date +"%Y%m%d%H%M%S"`
	cp $O_DIR/atos_mw_servidores.def $O_DIR/atos_mw_servidores_$FECHA.def
	cat $O_DIR/ac_reAdmComponentes_f > $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reAdmGranPublico_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reAdmXAGranPublico_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reAdmXaComponentes_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reCatalogoAtos_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reCodConfVisual_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reEstructurasAltasGranPublico_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reEstructurasEMP_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reEstructurasGestorNegocios_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reEstructurasGranPublico_f >> $O_DIR/atos_mw_servidores.def
	cat $O_DIR/ac_reFichaClienteRSC_f >> $O_DIR/atos_mw_servidores.def
